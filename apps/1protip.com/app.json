{
  "expo": {
    "name": "1ProTip",
    "slug": "1protip",
    "version": "1.0.1",
    "orientation": "portrait",
    "icon": "./assets/images/logo.png",
    "scheme": "oneprotip",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#0066cc"
    },
    "assetBundlePatterns": [
      "assets/**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.michaelsimoneau.oneprotip",
      "buildNumber": "2",
      "infoPlist": {
        "NSCameraUsageDescription": "This app uses the camera to capture photos for your posts.",
        "NSPhotoLibraryUsageDescription": "This app accesses your photo library to select images for your posts.",
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "package": "com.michaelsimoneau.oneprotip",
      "versionCode": 2,
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/logo.png",
        "backgroundColor": "#0066cc"
      },
      "permissions": [
        "android.permission.CAMERA",
        "android.permission.READ_MEDIA_IMAGES",
        "android.permission.RECORD_AUDIO"
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favlogo.png"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      "expo-web-browser",
      [
        "expo-camera",
        {
          "cameraPermission": "Allow 1ProTip to access your camera to capture photos for posts."
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "0ce2f32f-83bf-4499-8fe6-631239bc2b10"
      }
    },
    "updates": {
      "url": "https://u.expo.dev/0ce2f32f-83bf-4499-8fe6-631239bc2b10"
    },
    "owner": "michael-simoneau",
    "runtimeVersion": {
      "policy": "appVersion"
    }
  }
}
