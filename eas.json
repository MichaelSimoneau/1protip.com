{
  "cli": {
    "version": ">= 10.0.0",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "channel": "development",
      "ios": {
        "resourceClass": "m-medium",
        "bundleIdentifier": "com.michaelsimoneau.oneprotip"
      },
      "android": {
        "buildType": "apk",
        "package": "com.michaelsimoneau.oneprotip"
      },
      "env": {
        "EXPO_PUBLIC_APP_VARIANT": "development",
        "NODE_VERSION": "22.14.0"
      },
      "cache": {
        "key": "development-cache",
        "cacheDirectories": [
          "node_modules",
          "apps/free/node_modules"
        ]
      }
    },
    "development-simulator": {
      "developmentClient": true,
      "distribution": "internal",
      "channel": "development",
      "ios": {
        "simulator": true,
        "resourceClass": "m-medium",
        "bundleIdentifier": "com.michaelsimoneau.oneprotip"
      },
      "env": {
        "EXPO_PUBLIC_APP_VARIANT": "development",
        "NODE_VERSION": "22.14.0"
      },
      "cache": {
        "key": "development-simulator-cache",
        "cacheDirectories": [
          "node_modules",
          "apps/free/node_modules"
        ]
      }
    },
    "preview": {
      "distribution": "internal",
      "channel": "preview",
      "ios": {
        "resourceClass": "m-medium",
        "bundleIdentifier": "com.michaelsimoneau.oneprotip"
      },
      "android": {
        "buildType": "apk",
        "package": "com.michaelsimoneau.oneprotip"
      },
      "env": {
        "EXPO_PUBLIC_APP_VARIANT": "preview",
        "NODE_VERSION": "22.14.0"
      },
      "cache": {
        "key": "preview-cache",
        "cacheDirectories": [
          "node_modules",
          "apps/free/node_modules"
        ]
      }
    },
    "production": {
      "autoIncrement": true,
      "distribution": "store",
      "channel": "production",
      "ios": {
        "resourceClass": "m-medium",
        "bundleIdentifier": "com.michaelsimoneau.oneprotip"
      },
      "android": {
        "buildType": "app-bundle",
        "package": "com.michaelsimoneau.oneprotip"
      },
      "env": {
        "EXPO_PUBLIC_APP_VARIANT": "production",
        "NODE_VERSION": "22.14.0"
      },
      "cache": {
        "key": "production-cache",
        "cacheDirectories": [
          "node_modules",
          "apps/free/node_modules"
        ]
      }
    }
  },
  "submit": {
    "production": {
      "ios": {
        "appleId": "YOUR_APPLE_ID",
        "ascAppId": "YOUR_ASC_APP_ID",
        "appleTeamId": "YOUR_APPLE_TEAM_ID",
        "bundleIdentifier": "com.michaelsimoneau.oneprotip"
      },
      "android": {
        "serviceAccountKeyPath": "./google-services.json",
        "track": "production",
        "releaseStatus": "completed",
        "package": "com.michaelsimoneau.oneprotip"
      }
    }
  }
}
