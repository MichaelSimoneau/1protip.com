{
  "name": "@1protip/root",
  "version": "0.0.1",
  "description": "#1ProTip",
  "scripts": {
    "free": "pnpm --filter @1protip/free",
    "pro": "pnpm --filter @1protip/pro",
    "expo": "pnpm --filter @1protip/expo",
    "shared": "pnpm --filter @1protip/shared",
    "functions": "pnpm --filter @1protip/functions",
    "::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::": "",
    "start": "pnpm --parallel start",
    "dev": "pnpm --parallel dev",
    "typecheck": "pnpm --parallel typecheck",
    "lint": "pnpm --parallel lint",
    "test:e2e": "pnpm --filter @1protip/free test:selenium",
    ":::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::": "",
    "build": "pnpm --parallel build",
    "build:free": "pnpm free exec eas build --profile development --platform ios",
    "build:pro": "pnpm pro exec eas build --profile development --platform ios",
    "build:expo": "pnpm expo exec eas build --profile development --platform ios",
    "::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::": "",
    "deploy": "pnpm --parallel \"deploy:free\" \"deploy:pro\" \"deploy:expo\"",
    "deploy:free": "pnpm free exec eas deploy --profile development --platform ios",
    "deploy:pro": "pnpm pro exec eas deploy --profile development --platform ios",
    "deploy:expo": "pnpm expo exec eas deploy --profile development --platform ios",
    "::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::": "",
    "eas": "pnpm eas:free",
    "eas:free": "pnpm free exec eas",
    "eas:pro": "pnpm pro exec eas",
    "eas:expo": "pnpm expo exec eas"
  },
  "packageManager": "pnpm@10.25.0",
  "workspaces": [
    "apps/*",
    "functions/*",
    "shared/*"
  ],
  "dependencies": {
    "@expo/cli": "^54.0.19"
  },
  "devDependencies": {
    "firebase-tools": "^15.0.0"
  }
}
